{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "build": {
    "env": {},
    "commands": {
      "install": "npm install",
      "build": "npm run build"
    },
    "outputDirectory": "dist"
  }
}
